#!/bin/sh

if [ -n "$STP_TEST_DEBUG" ] ; then
    DEBUG=echo
fi

if [ "$STP_TEST_PROFILE" == checksums -o -z "$STP_TEST_SUITE" ] ; then
    $DEBUG /usr/bin/time ./run-testpattern-2 -q -M - "$@"
    $DEBUG /usr/bin/time ./run-testpattern-2 -F -s -q -M - -o PageSize,Resolution=MIN,DitherAlgorithm=VeryFast "$@"
else
    echo "Skipping in $STP_TEST_PROFILE suite"
    exit 77
fi
