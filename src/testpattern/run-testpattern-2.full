#!/bin/sh

if [ -n "$STP_TEST_DEBUG" ] ; then
    DEBUG=echo
fi

if [ -z "$STP_TEST_SUITE" -o -z "$STP_TEST_PROFILE" ] ; then
    STP_TEST_PROFILE=full
fi

if [ "$STP_TEST_PROFILE" == full ] ; then
    $DEBUG ./run-testpattern-2
    $DEBUG ./run-testpattern-2 -s -oPageSize,Resolution=MIN,DitherAlgorithm=VeryFast
else
    echo "Skipping in $STP_TEST_PROFILE suite"
    exit 77
fi
